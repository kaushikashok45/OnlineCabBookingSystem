      <servlet-name>Cab</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Driver.Cab</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Cab</servlet-name>  
     <url-pattern>/partner/*</url-pattern>  
    </servlet-mapping>
    <servlet>  
      <servlet-name>FetchLocations</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.EndUser.FetchLocations</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>FetchLocations</servlet-name>  
     <url-pattern>/fetchLocations/*</url-pattern>  
    </servlet-mapping>  
    <servlet>  
      <servlet-name>BookCab</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.EndUser.BookCab</servlet-class>  
