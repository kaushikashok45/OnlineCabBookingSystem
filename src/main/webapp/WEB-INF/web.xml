      <servlet-name>CancelTrip</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.EndUser.CancelTrip</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>CancelTrip</servlet-name>  
     <url-pattern>/CancelTrip/*</url-pattern>  
    </servlet-mapping> 
</web-app>