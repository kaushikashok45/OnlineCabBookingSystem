<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>com.tomcat_hello_world</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
    <servlet>  
      <servlet-name>SignUp</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.SignUp</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>SignUp</servlet-name>  
     <url-pattern>/signup/*</url-pattern>  
    </servlet-mapping>  
    <servlet>  
      <servlet-name>Login</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Login</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Login</servlet-name>  
     <url-pattern>/login/*</url-pattern>  
    </servlet-mapping> 
    <servlet>  
      <servlet-name>Account</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Account</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Account</servlet-name>  
     <url-pattern>/account</url-pattern>  
    </servlet-mapping> 
    <filter>
      <filter-name>AuthFilter</filter-name>
      <filter-class>com.tomcat_hello_world.Security.AuthFilter</filter-class>
    </filter>
    <filter-mapping>
      <filter-name>AuthFilter</filter-name>
      <url-pattern>/account</url-pattern>
    </filter-mapping>
    <servlet>  
      <servlet-name>Logout</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Logout</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Logout</servlet-name>  
     <url-pattern>/logout/*</url-pattern>  
    </servlet-mapping>
    <servlet>  
      <servlet-name>Auth</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Auth</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Auth</servlet-name>  
     <url-pattern>/auth</url-pattern>  
    </servlet-mapping>  
     <servlet>  
      <servlet-name>Process</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Process</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Process</servlet-name>  
     <url-pattern>/process/*</url-pattern>  
    </servlet-mapping> 
     <servlet>  
      <servlet-name>Admin</servlet-name>  
      <servlet-class>com.tomcat_hello_world.User.Administrator.Admin</servlet-class>  
    </servlet>  
    <servlet-mapping>  
     <servlet-name>Admin</servlet-name>  
     <url-pattern>/admin</url-pattern>  
    </servlet-mapping>
